{
    "name": "mot/ipdelete",
    "type": "phpbb-extension",
    "description": "Extension to delete IP addresses related to a phpBB user to be deleted",
    "homepage": "https://www.mike-on-tour.com",
    "version": "1.0.0",
    "time": "2020-09-30 08:12:11",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Michael Vormwald",
            "email": "webmaster@mike-on-tour.de",
            "homepage": "https://www.mike-on-tour.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpbb/phpbb": ">=3.2",
        "composer/installers": "~1.0.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "IP Address Deletion",
        "soft-require": {
            "phpbb/phpbb": ">=3.2"
        },
        "version-check": {
			"host": "www.mike-on-tour.com",
			"directory": "/mot",
			"filename": "mot_ipdelete.json",
            "ssl": true
        }
    }
}
